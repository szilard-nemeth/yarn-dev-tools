{
  "jobName": "Reviewsync",
  "commandType": "REVIEWSYNC",
  "envSanitizeExceptions": ["BRANCHES"],
  "mandatoryEnvVars": [
    "GSHEET_CLIENT_SECRET",
    "GSHEET_WORKSHEET",
    "GSHEET_SPREADSHEET",
    "GSHEET_JIRA_COLUMN",
    "GSHEET_UPDATE_DATE_COLUMN",
    "GSHEET_STATUS_INFO_COLUMN",
    "MAIL_ACC_USER",
    "MAIL_ACC_PASSWORD",
    "BRANCHES"
  ],
  "optionalEnvVars": [],
  "yarnDevToolsArguments": [
    "--debug",
    "REVIEWSYNC",
    "--gsheet",
    "--gsheet-client-secret ENV(GSHEET_CLIENT_SECRET)",
    "--gsheet-worksheet ENV(GSHEET_WORKSHEET)",
    "--gsheet-spreadsheet ENV(GSHEET_SPREADSHEET)",
    "--gsheet-jira-column ENV(GSHEET_JIRA_COLUMN)",
    "--gsheet-update-date-column ENV(GSHEET_UPDATE_DATE_COLUMN)",
    "--gsheet-status-info-column ENV(GSHEET_STATUS_INFO_COLUMN)",
    "--branches ENV(BRANCHES)"
  ],
  "globalVariables": {
    "sender": "YARN reviewsync",
    "subject": "YARN reviewsync report [start date: $$JOB_START_DATE$$]",
    "commandDataFileName": "command_data_$$JOB_START_DATE$$.zip"
  },
  "runs": [
    {
      "name": "run1",
      "variables": {
      },
      "emailSettings": {
        "enabled": true,
        "sendAttachment": false,
        "attachmentFileName": "$$commandDataFileName$$",
        "emailBodyFileFromCommandData": "report-short.html",
        "sender": "YARN reviewsync",
        "subject": "$$subject$$"
      },
      "driveApiUploadSettings": {
        "enabled": true,
        "fileName": "$$commandDataFileName$$"
      },
      "yarnDevToolsArguments": [
      ]
    }
  ]
}