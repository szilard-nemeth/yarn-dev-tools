{
  "jobName": "Reviewsync",
  "commandType": "REVIEWSYNC",
  "mandatoryEnvVars": ["GSHEET_CLIENT_SECRET", "GSHEET_SPREADSHEET", "MAIL_ACC_USER"],
  "optionalEnvVars": ["BRANCHES", "GSHEET_JIRA_COLUMN"],
  "mapEnvVarsToYarnDevToolsArgument": {
    "--gsheet-client-secret": "GSHEET_CLIENT_SECRET",
    "--gsheet-spreadsheet": "GSHEET_SPREADSHEET",
    "--gsheet-jira-column": "GSHEET_JIRA_COLUMN"
  },
  "yarnDevToolsArguments": [
    "--debug",
    "REVIEWSYNC",
    "--gsheet",
    "--gsheet-client-secret $$",
    "--gsheet-spreadsheet $$",
    "--gsheet-jira-column $$"
  ],
  "variables": {
    "algorithm": "testAlgorithm",
    "varA": "$$varB$$",
    "varB": "$$varC$$",
    "varC": "$$varD$$",
    "varD": "$$varE$$",
    "varE": "no_var_here",
    "varU": "$$varT$$",
    "varT": "$$varZ$$$$varS$$",
    "varS": "s",
    "varZ": "$$varX$$$$varY$$",
    "varX": "x",
    "varY": "y"
  }
}