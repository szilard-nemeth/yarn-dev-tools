{
  "jobName": "Reviewsync",
  "commandType": "REVIEWSYNC",
  "mandatoryEnvVars": [
    "GSHEET_CLIENT_SECRET",
    "GSHEET_SPREADSHEET",
    "MAIL_ACC_USER"
  ],
  "optionalEnvVars": [
    "BRANCHES",
    "GSHEET_JIRA_COLUMN"
  ],
  "yarnDevToolsArguments": [
    "--debug",
    "REVIEWSYNC",
    "--gsheet",
    "--algo $$algorithm$$",
    "--command-data-filename $$commandDataFileName$$"
  ],
  "global_variables": {
    "algorithm": "testAlgorithm",
    "commandDataFileName": "command_data_$$algorithm$$_$$JOB_START_DATE$$.zip"
  },
  "emailSettings": {
    "enabled": false,
    "sendAttachment": true,
    "attachmentFileName": "attachmentFileName",
    "subject": "testSubject"
  },
  "driveApiUploadSettings": {
    "enabled": false,
    "fileName": "simple"
  },
  "runs": [
    {
      "name": "dummy",
      "variables": {},
      "yarnDevToolsArguments": [
        "--arg1",
        "--arg2 param1 param2",
        "--arg3 param1",
        "--arg4"
      ]
    }
  ]
}