{
  "jobName": "Reviewsync",
  "commandType": "REVIEWSYNC",
  "mandatoryEnvVars": ["GSHEET_CLIENT_SECRET", "GSHEET_SPREADSHEET", "MAIL_ACC_USER"],
  "optionalEnvVars": ["BRANCHES", "GSHEET_JIRA_COLUMN"],
  "yarnDevToolsArguments": [
    "--gsheet-client-secret"
  ],
  "emailSettings": {
    "enabled": false,
    "sendAttachment": true,
    "attachmentFilename": "attachmentFilename+$$testVar3$$+$$testVar4$$",
    "subject": "testSubject+$$testVar2$$+$$testVar1$$"
  },
  "variables": {
    "testVar1": "v1",
    "testVar2": "v2",
    "testVar3": "v3",
    "testVar4": "v4"
  }
}